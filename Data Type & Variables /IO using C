#include <stdio.h>

int main()
{
    float num;
    double numDouble;
    
    printf("Enter a float ");
    scanf("%f", &num);
    
    printf("Enter a double ");
    scanf("%lf", &numDouble);
    
    printf("Float Number found is: %f\n", num);
    printf("Double Number found is: %lf", numDouble);
    return 0;
}
